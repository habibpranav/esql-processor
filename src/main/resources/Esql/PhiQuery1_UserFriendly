From:
sales
SELECT ATTRIBUTE(S):
cust, avg(1.quant), avg(2.quant), avg(3.quant)
NUMBER OF GROUPING VARIABLES(n):
3
GROUPING ATTRIBUTES(V):
cust
F-VECT([F]):
avg(1.quant), avg(2.quant), avg(3.quant)
SELECT CONDITION-VECT([Ïƒ]):
1.cust=cust and 1.state='NY'
2.cust=cust and 2.state='CT'
3.cust=cust and 3.state='NJ'
HAVING_CONDITION(G):
avg(1.quant) > avg(2.quant) and avg(1.quant) > avg(3.quant)